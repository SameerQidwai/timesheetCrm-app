{
  "name": "onelmApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start -- --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "debug-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "debug-apk": "cd android && ./gradlew assembleDebug && cd ..",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-*/ && rm -rf ./android/app/src/main/res/raw/ && rm -rf ./android/app/src/main/assets/index.android.bundle && rm -rf ./android/app/build/intermediates/assets/release/mergeReleaseAssets/index.android.bundle" ,
    "release-apk": "cd android && ./gradlew assembleRelease && cd .."
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^6.3.2",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@types/react-native-calendars": "^1.1264.3",
    "axios": "^0.27.2",
    "moment": "^2.29.4",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-calendars": "^1.1288.0",
    "react-native-document-picker": "^8.1.1",
    "react-native-dropdown-picker": "^5.4.2",
    "react-native-image-crop-picker": "^0.38.0",
    "react-native-mmkv": "^2.4.3",
    "react-native-modern-datepicker": "^1.0.0-beta.91",
    "react-native-paper": "^4.12.4",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.17.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^9.2.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
